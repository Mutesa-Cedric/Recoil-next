{
  "name": "recoil-relay-next",
  "version": "0.2.0",
  "description": "recoil-relay-next helps Recoil Next perform type safe and efficient queries using GraphQL with Relay",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "peerDependencies": {
    "react-relay": ">=13.2.0",
    "recoil-next": "^0.2.0",
    "relay-runtime": ">=13.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.2.39",
    "@types/react-relay": "^16.0.0",
    "@types/relay-runtime": "^17.0.0",
    "@types/relay-test-utils": "^19.0.0",
    "react-test-renderer": "^19.1.0",
    "relay-test-utils": "^17.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.2.1",
    "tslib": "^2.6.0",
    "typescript": "^5.4.2",
    "vitest": "^2.1.8"
  },
  "repository": "https://github.com/Mutesa-Cedric/Recoil-next.git",
  "license": "MIT"
}
